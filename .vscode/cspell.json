{
  "dictionaryDefinitions": [
    {
      "name": "main",
      "path": "dictionary.txt"
    }
  ],
  "enableGlobDot": true,
  "enabledLanguageIds": [
    "git-commit",
    "ignore",
    "json",
    "jsonc",
    "markdown",
    "plaintext",
    "plist",
    "python",
    "restructuredtext",
    "text",
    "toml",
    "xml",
    "yaml",
    "yml"
  ],
  "ignorePaths": [
    "*.log",
    ".coverage",
    ".directory",
    ".doctrees",
    ".git",
    ".mypy_cache/",
    ".pytest_cache/",
    ".vscode/extensions.json",
    "__pycache__",
    "_build/**",
    "build/**",
    "dist/**",
    "docs/_build/**",
    "htmlcov/**",
    "node_modules/**",
    "man/**"
  ],
  "language": "en-GB",
  "languageSettings": [
    {
      "dictionaries": ["main"],
      "languageId": "*"
    }
  ]
